#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shinhuiseong/Projects/state-management-tools/node_modules/.pnpm/pnpm@7.9.0/node_modules/pnpm/bin/node_modules:/Users/shinhuiseong/Projects/state-management-tools/node_modules/.pnpm/pnpm@7.9.0/node_modules/pnpm/node_modules:/Users/shinhuiseong/Projects/state-management-tools/node_modules/.pnpm/pnpm@7.9.0/node_modules:/Users/shinhuiseong/Projects/state-management-tools/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/shinhuiseong/Projects/state-management-tools/node_modules/.pnpm/pnpm@7.9.0/node_modules/pnpm/bin/node_modules:/Users/shinhuiseong/Projects/state-management-tools/node_modules/.pnpm/pnpm@7.9.0/node_modules/pnpm/node_modules:/Users/shinhuiseong/Projects/state-management-tools/node_modules/.pnpm/pnpm@7.9.0/node_modules:/Users/shinhuiseong/Projects/state-management-tools/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/pnpm.cjs" "$@"
else
  exec node  "$basedir/../../bin/pnpm.cjs" "$@"
fi
